{
  "name": "baiao-food",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "ts-build": "tsc",
    "tailwindcss": "tailwindcss -i ./src/input.css -o ./public/stylesheets/style.css",
    "build": "npm run ts-build && npm run tailwindcss",
    "server": "DEBUG=baiao-food:* nodemon ./bin/www",
    "dev": "concurrently \"npm:tailwindcss --watch -m\" \"npm:server\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
